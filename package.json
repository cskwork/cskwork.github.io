{
  "name": "ionic-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "_comment": " Translation not required : prebuild: npm run generate-markdown && scripts/i18n.sh && (if not defined CROWDIN_PERSONAL_TOKEN (exit 0) else (npm run crowdin:sync))...",
    "prebuild": "npm run generate-markdown; scripts/i18n.sh; if [ -n \"$CROWDIN_PERSONAL_TOKEN\" ]; then npm run crowdin:sync; fi",
    "build": "npm run build:${VERCEL_ENV:-preview}",
    "build:preview": "docusaurus build --locale en",
    "build:production": "docusaurus build",
    "clear": "docusaurus clear",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "generate-markdown": "node scripts/native && concurrently \"node scripts/cli\" \"node scripts/release-notes\"",
    "serve": "docusaurus serve",
    "prestart": "npm run generate-markdown",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle --danger"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/mdx-loader": "2.4.0",
    "@docusaurus/plugin-client-redirects": "2.4.0",
    "@docusaurus/plugin-content-docs": "2.4.0",
    "@docusaurus/plugin-content-pages": "2.4.0",
    "@docusaurus/plugin-debug": "2.4.0",
    "@docusaurus/plugin-google-analytics": "2.4.0",
    "@docusaurus/plugin-google-gtag": "2.4.0",
    "@docusaurus/plugin-sitemap": "2.4.0",
    "@docusaurus/preset-classic": "2.4.0",
    "@docusaurus/theme-classic": "0.0.0-4192",
    "@docusaurus/theme-search-algolia": "2.4.0",
    "@ionic-internal/docusaurus-plugin-tag-manager": "^2.0.0",
    "@ionic-internal/ionic-ds": "^8.0.0",
    "@mdx-js/react": "^2.3.0",
    "@prismicio/client": "^6.7.3",
    "@prismicio/react": "^2.5.2",
    "@stackblitz/sdk": "^1.8.2",
    "@tippyjs/react": "^4.2.6",
    "clsx": "^1.2.1",
    "concurrently": "^8.0.1",
    "crowdin": "^3.5.0",
    "docusaurus-plugin-module-alias": "^0.0.2",
    "docusaurus-plugin-sass": "^0.2.3",
    "fs-extra": "^11.1.1",
    "modern-normalize": "^1.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.1",
    "sass": "^1.60.0",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.0",
    "@ionic/prettier-config": "^2.0.0",
    "@tsconfig/docusaurus": "^1.0.7",
    "@types/react": "^17.0.37",
    "esm": "^3.2.25",
    "html-loader": "^4.2.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  }
}
